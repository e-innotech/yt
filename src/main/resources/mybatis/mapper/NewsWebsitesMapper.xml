<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yt.cms.mapper.NewsWebsitesMapper" >
  <resultMap id="BaseResultMap" type="com.yt.cms.model.NewsWebsites" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="news_id" property="newsId" jdbcType="INTEGER" />
    <result column="websites_id" property="websitesId" jdbcType="INTEGER" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, news_id, websites_id
  </sql>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    <include refid="Base_Column_List" />
    from news_websites
    where id = #{id,jdbcType=INTEGER}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from news_websites
    where id = #{id,jdbcType=INTEGER}
  </delete>
  
  <!-- 批量新增 -->
	<insert id="insertBatch" parameterType="map">
		insert into news_websites (news_id,websites_id) values
		<foreach collection="websitesIds" item="item" index="index" 
			separator=",">
			(#{newsId,jdbcType=INTEGER},#{item,jdbcType=INTEGER})
		</foreach>
	</insert>
	
  <insert id="insertSelective" parameterType="com.yt.cms.model.NewsWebsites" >
    <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
          SELECT LAST_INSERT_ID()
      	</selectKey>
    insert into news_websites
    <trim prefix="(" suffix=")" suffixOverrides="," >
     
      <if test="newsId != null" >
        news_id,
      </if>
      <if test="websitesId != null" >
        websites_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
     
      <if test="newsId != null" >
        #{newsId,jdbcType=INTEGER},
      </if>
      <if test="websitesId != null" >
        #{websitesId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.yt.cms.model.NewsWebsites" >
    update news_websites
    <set >
      <if test="newsId != null" >
        news_id = #{newsId,jdbcType=INTEGER},
      </if>
      <if test="websitesId != null" >
        websites_id = #{websitesId,jdbcType=INTEGER},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  
</mapper>